{
  "name": "vite-react-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "preview": "vite preview",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "typecheck": "tsc --pretty --noEmit",
    "generate": "yarn scaffdog generate",
    "codegen:graphql": "DOTENV_CONFIG_PATH=.env graphql-codegen -r dotenv/config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.10.2",
    "@chakra-ui/icons": "^2.0.4",
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@urql/exchange-auth": "^0.1.7",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "framer-motion": "^6.5.1",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-swipeable": "^7.0.0",
    "recharts": "^2.1.13",
    "recoil": "^0.7.4",
    "swr": "^1.3.0",
    "urql": "^2.2.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.0",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-urql": "^3.6.2",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/eslint": "^8.4.5",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/prettier": "^2.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-access": "^1.0.1",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-no-relative-import-paths": "^1.4.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.1",
    "eslint-plugin-strict-dependencies": "^1.0.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-scaffdog": "^1.5.0",
    "scaffdog": "^1.5.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "vite": "^3.0.3"
  }
}
